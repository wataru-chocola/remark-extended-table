{
  "name": "micromark-extension-extended-table",
  "version": "0.0.1",
  "description": "micromark extension to support table syntax allowing colspan / rowspan",
  "author": "Wataru Watanabe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wataru-chocola/micromark-extension-extended-table.git"
  },
  "bugs": {
    "url": "https://github.com/wataru-chocola/micromark-extension-extended-table/issues"
  },
  "homepage": "https://github.com/wataru-chocola/micromark-extension-extended-table",
  "main": "./index.js",
  "types": "./index.d.ts",
  "type": "module",
  "files": [
    "dev/**/*.js",
    "dev/**/*.d.ts",
    "!dev/**/*.test.js",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "!lib/**/*.test.js",
    "index.d.ts",
    "index.js"
  ],
  "exports": {
    "development": "./dev/index.js",
    "default": "./index.js"
  },
  "dependencies": {
    "micromark-util-chunked": "^1.0.0",
    "micromark-util-symbol": "^1.0.0",
    "micromark-util-types": "^1.0.1",
    "assert": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/node": "16.7.10",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "debug": "4.3.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "jest": "27.2.4",
    "micromark": "3.0.5",
    "micromark-build": "1.0.0",
    "micromark-extension-gfm-table": "^1.0.2",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "typescript": "4.4.3"
  },
  "peerDependencies": {
    "micromark": "^3.0.5"
  },
  "keywords": [
    "micromark",
    "micromark-extension"
  ],
  "scripts": {
    "format": "prettier --write \"./dev/**/*.{js,ts,json}\"",
    "lint": "eslint ./dev/**/*.{ts}",
    "lint-fix": "eslint --fix ./dev/**/*.{ts} && prettier --write ./dev/**/*.{ts}",
    "test": "npm run build && NODE_OPTIONS=--experimental-vm-modules jest --testPathIgnorePatterns=\"/node_modules/|/dev/\"",
    "test-dev": "tsc && NODE_OPTIONS=\"--experimental-vm-modules -C development\" jest \"dev/.*\\.test\\.js\"",
    "build": "npm run clean && tsc && micromark-build",
    "prepack": "npm run build",
    "clean": "rimraf lib/ index.js index.test.js dev/**/*.js"
  }
}
