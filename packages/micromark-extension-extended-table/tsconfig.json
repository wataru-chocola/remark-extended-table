{
  "include": ["./dev/**/*.ts"],
  "exclude": ["./dev/**/*.test.ts"],
  "compilerOptions": {
    "target": "es2018",
    "strict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "Bundler",
    "composite": true
  }
}
