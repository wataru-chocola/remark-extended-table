{
  "name": "mdast-util-extended-table",
  "version": "1.1.0",
  "description": "mdast extension for table allowing colspan/rowspan",
  "author": "Wataru Watanabe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wataru-chocola/remark-extended-table.git"
  },
  "bugs": {
    "url": "https://github.com/wataru-chocola/remark-extended-table/issues"
  },
  "homepage": "https://github.com/wataru-chocola/remark-extended-table/tree/main/packages/mdast-util-extended-table",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "!lib/*.test.js",
    "!lib/*.test.d.ts"
  ],
  "keywords": [
    "mdast",
    "mdast-util",
    "markdown"
  ],
  "dependencies": {
    "@types/hast": "^2.3.4",
    "@types/mdast": "^3.0.10",
    "@types/unist": "^2.0.6",
    "mdast-util-from-markdown": "^1.0.0",
    "mdast-util-to-hast": "^12.0.0",
    "mdast-util-to-markdown": "^2.0.0",
    "micromark-extension-extended-table": "^1.1.0",
    "unist-builder": "^3.0.0",
    "unist-util-position": "^4.0.1"
  },
  "devDependencies": {
    "hast-util-to-html": "8.0.4",
    "mdast-util-gfm-table": "1.0.7",
    "unist-util-inspect": "7.0.2"
  },
  "peerDependencies": {
    "mdast-util-gfm-table": "^1.0.1"
  },
  "scripts": {
    "format": "prettier --write \"./**/*.{ts,json}\"",
    "lint": "eslint ./**/*.{ts}",
    "lint-fix": "eslint --fix ./**/*.{ts} && prettier --write ./**/*.{ts,json}",
    "test": "pnpm run build && NODE_OPTIONS=--experimental-vm-modules jest 'test\\.js' -e --testPathIgnorePatterns=\"/node_modules/\"",
    "build": "tsc --build --verbose",
    "rebuild": "pnpm run clean && pnpm run build",
    "prepublish": "pnpm run rebuild",
    "clean": "rimraf lib/*{.js,.d.ts} tsconfig.tsbuildinfo"
  }
}
